# Real Estate Lead Generation AI - Environment Configuration
# Copy this file to .env and fill in your API keys

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Real Estate APIs (Optional)
ZILLOW_API_KEY=your_zillow_api_key_here
REALTOR_API_KEY=your_realtor_api_key_here
MLS_API_KEY=your_mls_api_key_here

# Skiptracing APIs (Optional)
SPOKEO_API_KEY=your_spokeo_api_key_here
TRUEPEOPLESEARCH_API_KEY=your_truepeoplesearch_api_key_here
WHITEPAGES_API_KEY=your_whitepages_api_key_here

# CRM Integration (Optional)
GOOGLE_SHEETS_CREDENTIALS_FILE=path_to_google_sheets_credentials.json
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here

# Workflow Configuration
MAX_LEADS_PER_SEARCH=50
MIN_LEAD_SCORE=30
REQUIRE_HUMAN_REVIEW=true
AUTO_EXPORT=false

# Output Configuration
OUTPUT_DIRECTORY=./outputs
DEFAULT_OUTPUT_FORMAT=csv

# Streamlit Configuration (Optional)
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost
